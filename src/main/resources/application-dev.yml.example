spring:
  datasource:
    url: ${DB_URL:jdbc:postgresql://localhost:5432/moneyflow_dev}
    username: ${DB_USER:postgres}
    password: ${DB_PASSWORD:password}
    driver-class-name: org.postgresql.Driver

  jpa:
    hibernate:
      ddl-auto: none
    show-sql: true


  flyway:
    enabled: false

server:
  port: 8080

logging:
  level:
    com.moneyflow: DEBUG
